        window.addEventListener('DOMContentLoaded', function() {
          var tweets = JSON.parse(json) || [];
          var tweetsFeed = '';
          slicedTweets = tweets.slice(0, 3);

          for(key in slicedTweets){
            var text = replaceURLWithHTMLLinks(slicedTweets[key].text);
            var name = slicedTweets[key].user.screen_name;
            var date = H(slicedTweets[key].created_at);
            tweetsFeed += '<li class="content-list__item content-list__item_devided company-tweet">\
              <div class="company-tweet__head"><a href="https://twitter.com/'+name+'" class="company-tweet__username">'+name+'</a><span class="company-tweet__date">'+date+'</span></div>\
              <div class="company-tweet__text">'+text+'</div>\
            </li>'
          }
          $('#tweets_stream').append(tweetsFeed);
        }, true);
      </script>
